{
  "name": "dlx-studios-ultimate",
  "version": "1.0.0",
  "description": "DLX Studios Ultimate - AI-Native Development Platform with VibeEditor",
  "main": "dist-electron/main.js",
  "type": "module",
  "scripts": {
    "predev": "npx --yes kill-port 5173 || exit 0",
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "electron:dev": "concurrently \"npm run dev\" \"wait-on tcp:localhost:5173 && cross-env NODE_ENV=development tsx electron/main.ts\"",
    "electron:build:main": "tsc -p tsconfig.electron.json",
    "electron:build:all": "npm run build && npm run electron:build:main && electron-builder",
    "electron:build": "npm run build && npm run electron:build:main && electron-builder",
    "electron:pack": "electron-builder",
    "typecheck": "tsc --noEmit",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "format": "prettier --write \"src/**/*.{ts,tsx,css}\"",
    "test": "vitest",
    "test:e2e": "playwright test"
  },
  "keywords": [
    "ai",
    "code-editor",
    "electron",
    "development",
    "vibdee",
    "llm"
  ],
  "author": "DLX Studios",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Dunker007/11-6.git"
  },
  "bugs": {
    "url": "https://github.com/Dunker007/11-6/issues"
  },
  "homepage": "https://github.com/Dunker007/11-6#readme",
  "devDependencies": {
    "@playwright/test": "^1.56.1",
    "@types/node": "^24.10.0",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "@typescript-eslint/eslint-plugin": "^8.46.3",
    "@typescript-eslint/parser": "^8.46.3",
    "@vitejs/plugin-react": "^5.1.0",
    "@vitest/ui": "^4.0.7",
    "concurrently": "^9.2.1",
    "cross-env": "^10.1.0",
    "electron": "^39.1.1",
    "electron-builder": "^26.0.12",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-react-hooks": "^7.0.1",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.6",
    "playwright": "^1.56.1",
    "prettier": "^3.6.2",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3",
    "vite": "^7.2.1",
    "vitest": "^4.0.7",
    "wait-on": "^9.0.1"
  },
  "dependencies": {
    "@dnd-kit/core": "^6.3.1",
    "@google/generative-ai": "^0.24.1",
    "@monaco-editor/react": "^4.7.0",
    "@octokit/rest": "^22.0.1",
    "@types/react-grid-layout": "^1.3.5",
    "chokidar": "^4.0.3",
    "electron-updater": "^6.1.7",
    "lucide-react": "^0.553.0",
    "monaco-editor": "^0.54.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-grid-layout": "^1.5.2",
    "react-markdown": "^10.1.0",
    "simple-git": "^3.30.0",
    "systeminformation": "^5.27.11",
    "zustand": "^5.0.8"
  }
}
