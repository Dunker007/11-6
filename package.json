  "description": "DLX Studios Ultimate - AI-Native Development Platform with VibeEditor",
  "main": "dist-electron/main.js",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "electron:dev": "concurrently \"npm run dev\" \"wait-on http://localhost:4173 && cross-env NODE_ENV=development tsx electron/main.ts\"",
    "electron:build:main": "tsc -p tsconfig.electron.json",
    "electron:build:all": "npm run build && npm run electron:build:main && electron-builder",
    "electron:build": "npm run build && npm run electron:build:main && electron-builder",
    "electron:pack": "electron-builder",
    "typecheck": "tsc --noEmit",
    "lint": "eslint . --report-unused-disable-directives --max-warnings 0",
    "format": "prettier --write \"src/**/*.{ts,tsx,css}\"",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "test:e2e": "playwright test"
  },
  "keywords": [
    "ai",
    "code-editor",
    "electron",
    "development",
    "vibdee",
    "llm"
  ],
